{
    "installCommand": "bash check_deploy.sh",
    "redirects": [
        {
            "source": "/",
            "destination": "/index.html",
            "permanent": false
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/public/$1"
        }
    ]
}